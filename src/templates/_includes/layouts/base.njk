      </symbol>
      <symbol id="icon-close" viewBox="0 0 32 32" style="overflow: visible;">
        <path d="M0, 0 L32, 32 M32, 0 L0, 32"></path>
      </symbol>
      <symbol id="icon-play" viewBox="0 0 24 20" style="overflow: visible;">
        <path d="M4 0L24 10L4 20V0z"/>
      </symbol>
      <symbol id="icon-pause" viewBox="0 0 24 20" style="overflow: visible;">
        <rect width="7" height="20" x="2"/>
        <rect width="7" height="20" x="15"/>
      </symbol>
    </svg>
    {# <div class="site-container"> #}
    <header class="header">
      {# <div class="header-inner"> #}
      <a class="header-logo" href="{{ '/' | url }}" title="Home">
        {# <div class="header-logo-text"> #}
              Charles Robertson
            {# </div> #}</a>

      {#- Read more about `eleventy-navigation` at https://github.com/11ty/eleventy-navigation #}
      <nav class="header-nav">
        <ul class="header-nav-list">
          {% for navItem in navItems %}
            <li class="header-nav-item {%- if (navItem.slug | isActive(page.fileSlug, tags[0])) %} current{% endif %}">
              <a href="/{{ navItem.slug }}/">
                <div class="header-nav-item-inner">
                  {{ navItem.title }}
                </div>
              </a>
            </li>
          {% endfor %}
        </ul>
      </nav>
      {# </div> #}
    </header>

    <main {% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

    {%- include "post-nav.njk" -%}
    {%- include "footer.njk" -%}

    <script defer>
      document.querySelector('body').classList.remove('no-js');
    </script>
  </body>
</html>